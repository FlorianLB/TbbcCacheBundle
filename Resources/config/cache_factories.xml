<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
      <service id="kitano_cache.cache_factory.memcached" class="Kitano\CacheBundle\DependencyInjection\CacheFactory\MemcachedCacheFactory">
        <tag name="kitano_cache.cache_factory" cache_type="memcached" />
      </service>
      <service id="kitano_cache.cache_factory.apc" class="Kitano\CacheBundle\DependencyInjection\CacheFactory\ApcCacheFactory">
        <tag name="kitano_cache.cache_factory" cache_type="apc" />
      </service>
    </services>

</container>
